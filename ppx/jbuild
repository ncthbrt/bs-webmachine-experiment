(jbuild_version 1)

(library
 ((name reconstruct_ppx)  
  (synopsis "Compiler extensions to make writing web servers typesafe")
  (kind ppx_rewriter)
  (ppx_runtime_libraries (reconstruct str))
  (preprocess (pps (ppxlib.metaquot)))
  #; (flags (:standard -w +A-4-42-40))
